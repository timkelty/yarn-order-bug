{
  "name": "inspect-f",
  "version": "1.2.1",
  "description": "Cast a function to string and adjust its indentation",
  "main": "inspect-f.js",
  "scripts": {
    "clean": "rimraf npm-debug.log coverage",
    "lint": "eslint inspect-f.js test",
    "lint:fix": "npm run lint -- --fix",
    "release": "npm outdated --long && xyz --edit --repo git@github.com:fluture-js/inspect-f.git --tag 'X.Y.Z' --increment",
    "test": "npm run test:all && npm run test:coverage && codecov",
    "test:all": "npm run lint && npm run test:unit",
    "test:unit": "_mocha --ui bdd --reporter list --check-leaks --full-trace test/**.test.js",
    "test:coverage": "npm run clean && istanbul cover --report html _mocha -- --ui bdd --reporter dot --check-leaks --bail test/**.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fluture-js/inspect-f.git"
  },
  "keywords": [
    "cast",
    "inspect",
    "function",
    "toString",
    "show",
    "indent"
  ],
  "author": "Aldwin Vlasblom <aldwin.vlasblom@gmail.com> (https://github.com/Avaq)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fluture-js/inspect-f/issues"
  },
  "homepage": "https://github.com/fluture-js/inspect-f",
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "eslint": "^3.12.2",
    "eslint-config-warp": "^1.2.0",
    "istanbul": "^0.4.2",
    "jsverify": "^0.7.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.2",
    "xyz": "^2.0.1"
  }
}
